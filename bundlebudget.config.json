{
    "performance-budget": {
      "default": {
        "maxSize": "250kb",
        "maxGzippedSize": "80kb"
      },
      "global": {
        "maxScriptSize": "300kb",
        "maxStylesheetSize": "100kb"
      },
      "rules": [
        {
          "name": "main.js",
          "maxSize": "150kb"
        },
        {
          "name": "vendor.js",
          "maxSize": "200kb"
        },
        {
          "name": "styles.css",
          "maxSize": "50kb"
        }
      ],
      "warnings": {
        "overBudgetBy": "10%"
      }
    }
  }